{
    "name": "turtlebytes/fire",
    "description": "Work with the main task management systems like jira, azure, and aha",
    "keywords": [
        "jira",
        "azure",
        "aha",
        "task management management",
        "console",
        "cli"
    ],
    "homepage": "https://github.com/zach2825/dumpster-fire",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zach Robichaud",
            "email": "zach2825@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "doctrine/dbal": ">=3.1",
        "guzzlehttp/guzzle": ">=6.5.5",
        "illuminate/database": ">=8.0",
        "illuminate/http": ">=8.0",
        "intonate/tinker-zero": ">=1.2",
        "laminas/laminas-text": ">=2.8",
        "laravel-zero/framework": ">=8.7",
        "laravel/pint": "^1.2",
        "thenetworg/oauth2-azure": ">=2.0"
    },
    "require-dev": {
        "fakerphp/faker": ">=1.9.1",
        "mockery/mockery": ">=1.4.2",
        "pestphp/pest": ">=1.0",
        "rector/rector": "^0.14.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "cleanup": "pint"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "taskr"
    ]
}
